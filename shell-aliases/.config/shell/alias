alias c="clear"
alias v="nvim"
alias cat="bat"
alias ls="ls -h --color=auto --group-directories-first"
alias la="ls -ah --color=auto --group-directories-first"
alias ll="ls -lah --color=auto --group-directories-first"
alias grep="grep --color=auto"
alias mv="mv -i"
alias rm="rm -Iv"
alias p="ps aux | grep $1"
